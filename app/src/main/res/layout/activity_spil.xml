<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mrmini.hold1e17.dk.mrmini.Spil"
    android:id="@+id/view_root"
    android:background="@drawable/magnetgameback">


    <ImageView
        android:id="@+id/iv1"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="88dp"
        android:layout_marginTop="15dp"
        app:srcCompat="@drawable/magnet5"
        tools:layout_width="90dp" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/iv1"
        android:layout_marginStart="12dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/magnet4" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="25dp"
        android:layout_marginEnd="19dp"
        app:srcCompat="@drawable/magnet3" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/iv3"
        android:layout_below="@+id/iv2"
        app:srcCompat="@drawable/goldmedal" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/iv2"
        android:layout_below="@+id/iv2"
        android:layout_marginTop="18dp"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/iv6"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignTop="@+id/iv2"
        android:layout_marginTop="16dp"
        android:layout_toStartOf="@+id/iv3"
        app:srcCompat="@drawable/magnet77" />

</RelativeLayout >
