<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mrmini.hold1e17.dk.mrmini">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-feature android:name="android.hardware.camera2"></uses-feature>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Scanner_appScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Hovedmenu"
            android:label="Hovedmenu"/>
        <activity
            android:name=".Login"
            android:label="Log på"/>
        <activity
            android:name=".HospitalsInfo"
            android:label="Hospitals Info"
            android:screenOrientation="portrait"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Hovedmenu" />
        <activity
            android:name=".Indstillinger"
            android:label="@string/settings"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Hovedmenu" />
        <activity
            android:name=".Scanner"
            android:label="@string/scanner"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Hovedmenu" />
        <activity android:name=".Spil"
            android:label="@string/spil"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Hovedmenu" />
        <activity android:name=".Scanner_Toy"
            android:label="Scan med legetøj"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Scanner"/>
        <activity android:name=".Scanner_app"
            android:label="Scan med app"
            android:parentActivityName="mrmini.hold1e17.dk.mrmini.Scanner_app"/>
    </application>

</manifest>
